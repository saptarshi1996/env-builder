{
  "dependencies": {
    "clipboardy": "^4.0.0",
    "commander": "^9.4.0"
  },
  "name": "build-sync",
  "description": "Keep env files in sync with this cli tool",
  "version": "2.5.0",
  "engines": {
    "npm": ">= 8.19.3",
    "node": ">= 18.13.0",
    "yarn": "disabled"
  },
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "build": "gulp build",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "spellcheck": "cspell src/",
    "prepare": "husky",
    "sync": "build-sync",
    "dev": "gulp build && node dist/index.js",
    "endev": "gulp build && node dist/index.js -en"
  },
  "bin": {
    "build-sync": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saptarshi1996/env-builder.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saptarshi1996/env-builder/issues"
  },
  "homepage": "https://github.com/saptarshi1996/env-builder#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.2.0",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-swc": "^1.3.2",
    "gulp-terser": "^2.1.0",
    "husky": "^9.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
